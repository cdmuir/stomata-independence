@article{galmes_leaf_2013,
	title = {Leaf responses to drought stress in {Mediterranean} accessions of {Solanum} lycopersicum: anatomical adaptations in relation to gas exchange parameters},
	volume = {36},
	issn = {1365-3040},
	shorttitle = {Leaf responses to drought stress in {Mediterranean} accessions of {Solanum} lycopersicum},
	url = {https://onlinelibrary.wiley.com/doi/abs/10.1111/pce.12022},
	doi = {10.1111/pce.12022},
	abstract = {In a previous study, important acclimation to water stress was observed in the Ramellet tomato cultivar (TR) from the Balearic Islands, related to an increase in the water-use efficiency through modifications in both stomatal (gs) and mesophyll conductances (gm). In the present work, the comparison of physiological and morphological traits between TR accessions grown with and without water stress confirmed that variability in the photosynthetic capacity was mostly explained by differences in the diffusion of CO2 through stomata and leaf mesophyll. Maximization of gm under both treatments was mainly achieved through adjustments in the mesophyll thickness and porosity and the surface area of chloroplasts exposed to intercellular airspace (Sc). In addition, the lower gm/Sc ratio for a given porosity in drought-acclimated plants suggests that the decrease in gm was due to an increased cell wall thickness. Stomatal conductance was also affected by drought-associated changes in the morphological properties of stomata, in an accession and treatment-dependent manner. The results confirm the presence of advantageous physiological traits in the response to drought stress in Mediterranean accessions of tomato, and relate them to particular changes in the leaf anatomical properties, suggesting specific adaptive processes operating at the leaf anatomical level.},
	language = {en},
	number = {5},
	urldate = {2020-07-20},
	journal = {Plant, Cell \& Environment},
	author = {Galmés, Jeroni and Ochogavía, Joan Manuel and Gago, Jorge and Roldán, Emilio José and Cifre, Josep and Conesa, Miquel Àngel},
	year = {2013},
	keywords = {photosynthesis, stomatal conductance, mesophyll conductance, tomato, water stress},
	pages = {920--935},
}

