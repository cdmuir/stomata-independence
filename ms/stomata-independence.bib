@article{franks_effect_2001,
	title = {The {Effect} of {Exogenous} {Abscisic} {Acid} on {Stomatal} {Development}, {Stomatal} {Mechanics}, and {Leaf} {Gas} {Exchange} in \textit{{Tradescantia} virginiana}},
	volume = {125},
